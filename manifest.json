{
  "manifest_version": 2,

  "name": "Live Soccer Feeds",
  "description": "Extension provides you live updates on your fav. leagues",
  "version": "1.0",

"app" : {
    "background" : {
	  "scripts" : ["app.js"]
    }
  },
    
  "permissions" : [
     "notifications", "http://*/*" , "storage", "app.window", "webview"
  ]
}
